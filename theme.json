{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [
				{
					"name": "Blue",
					"slug": "blue",
					"colors": ["#462CFF", "#ECE8FF"]
				}
			],
			"gradients": [
				{
					"name": "Light",
					"slug": "light",
					"gradient": "linear-gradient(135deg, #EAE6FD, #D0F0FF, #DDFBEF)"
				},
				{
					"name": "Dark",
					"slug": "dark",
					"gradient": "linear-gradient(135deg, #1E203D, #323562)"
				}
			],
			"palette": [
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#4d34fa"
				},
				{
					"name": "Main",
					"slug": "main",
					"color": "#1E203D"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "#636381"
				},
				{
					"name": "Main Accent",
					"slug": "main-accent",
					"color": "#d0ccde"
				},
				{
					"name": "Primary Accent",
					"slug": "primary-accent",
					"color": "#e4e1fe"
				},
				{
					"name": "Tertiary",
					"slug": "tertiary",
					"color": "#f6f6fc"
				},
				{
					"name": "Border Light",
					"slug": "border-light",
					"color": "#e2e6ea"
				},
				{
					"name": "Border Dark",
					"slug": "border-dark",
					"color": "#444B57"
				},
				{
					"name": "Base",
					"slug": "base",
					"color": "#fff"
				}
			],
			"link": true
		},
		"custom": {
			"fontWeight": {
				"thin": 100,
				"light": 200,
				"book": 300,
				"regular": 400,
				"medium": 500,
				"semi-bold": 600,
				"bold": 700,
				"extra-bold": 800,
				"black": 900
			},
			"lineHeight": {
				"none": 1,
				"tight": 1.1,
				"snug": 1.2,
				"body": 1.5,
				"relaxed": 1.625,
				"loose": 2
			}
		},
		"layout": {
			"contentSize": "740px",
			"wideSize": "1260px"
		},
		"spacing": {
			"spacingSizes": [
				{
					"name": "Small",
					"size": "clamp(.5rem, 2.5vw, 1rem)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.5rem, 4vw, 2rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(2rem, 5vw, 3rem)",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "clamp(3rem, 7vw, 5rem)",
					"slug": "x-large"
				},
				{
					"name": "2xl",
					"size": "clamp(4rem, 9vw, 7rem)",
					"slug": "xx-large"
				},
				{
					"name": "3xl",
					"size": "clamp(5rem, 12vw, 9rem)",
					"slug": "xxx-large"
				},
				{
					"name": "4xl",
					"size": "clamp(6rem, 14vw, 13rem)",
					"slug": "xxxx-large"
				}
			],
			"units": ["px", "em", "rem", "vh", "vw", "%"],
			"blockGap": true,
			"padding": true,
			"margin": true
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "Inter",
					"name": "inter",
					"slug": "inter",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Inter",
							"fontStretch": "75% 125%",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/Inter-VariableFont_opsz,wght.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "Tofino",
					"name": "tofino",
					"slug": "primary",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Mona Sans",
							"fontStretch": "75% 125%",
							"fontStyle": "normal",
							"fontWeight": "100",
							"src": [
								"file:./assets/fonts/TofinoWide-ThinPersonal.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "Tofino",
					"name": "tofino",
					"slug": "primary",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Mona Sans",
							"fontStretch": "75% 125%",
							"fontStyle": "normal",
							"fontWeight": "200",
							"src": [
								"file:./assets/fonts/TofinoWide-LightPersonal.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "Tofino",
					"name": "tofino",
					"slug": "primary",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Mona Sans",
							"fontStretch": "75% 125%",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/TofinoWide-BookPersonal.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "Tofino",
					"name": "tofino",
					"slug": "primary",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Mona Sans",
							"fontStretch": "75% 125%",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/TofinoWide-RegularPersonal.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "Tofino",
					"name": "tofino",
					"slug": "primary",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Mona Sans",
							"fontStretch": "75% 125%",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/TofinoWide-MediumPersonal.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "Tofino",
					"name": "tofino",
					"slug": "primary",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Mona Sans",
							"fontStretch": "75% 125%",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/TofinoWide-BoldPersonal.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "Tofino",
					"name": "tofino",
					"slug": "primary",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Mona Sans",
							"fontStretch": "75% 125%",
							"fontStyle": "normal",
							"fontWeight": "800",
							"src": [
								"file:./assets/fonts/TofinoWide-BlackPersonal.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "monospace",
					"name": "Monospace",
					"slug": "monospace"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": ".825rem",
						"max": ".95rem"
					},
					"size": ".95rem",
					"slug": "x-small",
					"name": "Extra Small"
				},
				{
					"fluid": {
						"min": ".9rem",
						"max": "1.05rem"
					},
					"size": "1.05rem",
					"slug": "small",
					"name": "Small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.165rem"
					},
					"size": "1.165rem",
					"slug": "base",
					"name": "Base"
				},
				{
					"fluid": {
						"min": "1.365rem",
						"max": "1.65rem"
					},
					"size": "1.65rem",
					"slug": "medium",
					"name": "Medium"
				},
				{
					"fluid": {
						"min": "1.65rem",
						"max": "2rem"
					},
					"size": "2rem",
					"slug": "mediumlarge",
					"name": "Medium Large"
				},
				{
					"fluid": {
						"min": "1.75rem",
						"max": "2.75rem"
					},
					"size": "2.75rem",
					"slug": "large",
					"name": "Large"
				},
				{
					"fluid": {
						"min": "2.425rem",
						"max": "3.5rem"
					},
					"size": "3.5rem",
					"slug": "x-large",
					"name": "Extra Large"
				},
				{
					"fluid": {
						"min": "3.25rem",
						"max": "4.3875rem"
					},
					"size": "4.3875rem",
					"slug": "xx-large",
					"name": "Extra Extra Large"
				}
			],
			"lineHeight": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/paragraph": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontWeight": "var(--wp--custom--font-weight--regular)"
				}
			},
			"core/list": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontWeight": "var(--wp--custom--font-weight--regular)"
				}
			},
			"core/navigation": {
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--medium)",
					"fontFamily": "var(--wp--preset--font-family--inter)"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/pullquote": {
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)"
						},
						"color": {
							"text": "var(--wp--preset--color--secondary)"
						}
					}
				},
				"border": {
					"color": "var(--wp--preset--color--primary)",
					"top": {
						"style": "solid",
						"width": "5px"
					}
				},
				"spacing": {
					"padding": {
						"top": "0",
						"right": "0",
						"bottom": "0",
						"left": "0"
					}
				},
				"typography": {
					"lineHeight": "var(--wp--custom--line-height--body)",
					"fontSize": "var(--wp--preset--font-size--regular)",
					"fontWeight": "var(--wp--custom--font-weight--medium)"
				}
			},
			"core/quote": {
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)"
						},
						"color": {
							"text": "var(--wp--preset--color--secondary)"
						}
					}
				},
				"css": "& :where(cite) { display: block }",
				"border": {
					"radius": "0",
					"style": "solid",
					"width": "0 0 0 5px !important",
					"color": "var(--wp--preset--color--primary)"
				},
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--large)",
						"right": "var(--wp--preset--spacing--large)"
					}
				},
				"typography": {
					"lineHeight": "var(--wp--custom--line-height--body)",
					"fontSize": "var(--wp--preset--font-size--regular)",
					"fontWeight": "var(--wp--custom--font-weight--medium)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)",
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"lineHeight": "var(--wp--custom--line-height--none)",
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"letterSpacing": "0"
				}
			},
			"core/table": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/template-part": {
				"spacing": {
					"margin": {
						"top": "0px !important"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--main)"
		},
		"elements": {
			"body": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "var(--wp--preset--font-size--base)",
					"lineHeight": "var(--wp--custom--line-height--body)"
				}
			},
			"button": {
				"border": {
					"radius": "3px",
					"width": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--base)"
				},
				"spacing": {
					"padding": {
						"top": "1em",
						"right": "2em",
						"bottom": "1em",
						"left": "2em"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "var(--wp--custom--font-weight--medium)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--main)",
						"text": "var(--wp--preset--color--base)"
					},
					"typography": {
						"textDecoration": "underline"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "var(--wp--custom--line-height--snug)",
					"fontWeight": "var(--wp--custom--font-weight--medium)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--mediumlarge)",
					"lineHeight": "var(--wp--custom--line-height--snug)",
					"fontWeight": "var(--wp--custom--font-weight--medium)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--mediumlarge)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontWeight": "var(--wp--custom--font-weight--semi-bold)",
					"lineHeight": "var(--wp--custom--line-height--body)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--main)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--medium)",
			"padding": {
				"top": "0",
				"right": "var(--wp--preset--spacing--medium)",
				"bottom": "0",
				"left": "var(--wp--preset--spacing--medium)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"fontWeight": "var(--wp--custom--font-weight--regular)",
			"lineHeight": "var(--wp--custom--line-height--body)"
		}
	}
}
